CREATE TABLE manager (
  id int(4) NOT NULL auto_increment,
  name varchar(255) default NULL,
  foto varchar(64) default NULL,
  slogan text,
  email varchar(255) default NULL,
  tel varchar(255) default NULL,
  srt int(4) default NULL,
  PRIMARY KEY  (id),
  KEY i_project (id)
) TYPE=MyISAM;
